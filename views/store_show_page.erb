<div class="container">
	<h1 class="text-center"><%= @store.name %></h1>
		<div class="row">
			<% @products.each do |product| %>
				<div class="col-md-4"><a href="/stores/<%= @store.name %>/<%= product.id %>"><img src="<%= product.image_url %>" class="img-responsive" class="img-responsive img-rounded"></a></div>
			<% end %>
		</div>
</div>