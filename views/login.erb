<div class="container">
  <div class="row customBox">
    <div class="col-md-4 col-md-offset-2">

    <h3 class="text-center">Log In</h3>

      <form action="/session" method="post">
        <div class="form-group">
          <label for="email">Email Address</label>
          <input type="text" class="form-control" name="email" placeholder="6ofSpades@email.com">
        </div>
        <div class="form-group">
          <label for="password">Password</label>
          <input type="password" class="form-control" name="password" placeholder="Password">
        </div>
        <button type="submit" class="btn btn-success">Log in</button>
      </form>
    </div>

    <div class="col-md-4 col-md-offset-1 text-center aside">
      <h3>Or sign up!</h3>
      <p>Create an account and save your favourite swag</p>
      <a href="/signup" class="btn btn-success">Sign up</a>
    </div>
  </div>
</div>